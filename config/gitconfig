[user]
	name = Keith Rollin
	email = keith.rollin@gmail.com
[init]
	defaultBranch = main
[push]
	default = simple
[alias]
	br = branch
	ci = commit
	co = checkout
	fix = commit -a --amend --no-edit
	h = help
	list-remote-branches = ls-remote --heads
	log1 = log -1
	logf = log --raw
	logl = log --oneline
	logp = log -p
	ls-alias = !git config --list | fgrep 'alias.' | sed 's/alias\\.\\([^=]*\\)=\\(.*\\)/\\1 => \\2/' | sort
	shorty = status --short --branch
	st = status
	unstage = reset HEAD --
	unstash = stash pop
	wip = commit -a -m wip
[color]
	branch = auto
	diff = auto
	status = auto
	interactive = auto
[merge]
	conflictstyle = diff3
[rerere]
	enabled = true
[sendemail]
	transferEncoding = base64
[log]
	date = local
[core]
	fsmonitor = true
[fetch]
	showForcedUpdates = true
	prune = true
[feature]
	manyfiles = 1
