	name = Keith Rollin
	email = keith.rollin@gmail.com
[push]
	default = simple
[alias]
	ci = commit
	br = branch
	wc = log --raw
	s = status -s
	st = status
	co = checkout
	lg = log -p
	lol = log --oneline
	h = help
	unstage = reset HEAD --
	last = log -1 HEAD
	spull = !git svn rebase
	spush = !git svn dcommit
[color]
	branch = auto
	diff = auto
	status = auto
	interactive = auto
[core]
	#whitespace = nowarn
[apply]
	#whitespace = nowarn
[merge]
	conflictstyle = diff3
[rerere]
	enabled = true
[credential]
	helper = osxkeychain
