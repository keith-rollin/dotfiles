[user]
	name = Keith Rollin
	email = keith.rollin@gmail.com
[push]
	default = simple
[alias]
	br = branch
	co = checkout
	ci = commit
	h = help
	unstage = reset HEAD --
	ls-alias = config --list | fgrep 'alias.' | sed 's/alias\\.\\([^=]*\\)=\\(.*\\)/\\1 => \\2/' | sort
	s = status -s
	st = status
	log1 = log -1 HEAD
	logf = log --raw
	logg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
	logl = log --oneline
	logp = log -p
	spull = !git svn rebase
	spush = !git svn dcommit
	srev = !sh -c 'git svn find-rev r$1 | xargs git show' -
[color]
	branch = auto
	diff = auto
	status = auto
	interactive = auto
[core]
	#whitespace = nowarn
[apply]
	#whitespace = nowarn
[merge]
	conflictstyle = diff3
[rerere]
	enabled = true
[credential]
	helper = osxkeychain
